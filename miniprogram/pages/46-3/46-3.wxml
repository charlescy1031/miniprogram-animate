        <nav showNav="{{showNav}}" navTitle="46-3"></nav>
    </view>

    <view class="animation-box" wx:if="{{showScreen}}" bindtap="clickfn" animation="{{animationData1}}" style="background:url(https://gd-hbimg.huaban.com/ed2daada74197bdebb8de43b4c74cb741fbe0d63c02bd-WZpvUw);background-size: 100%;">
        <view class="container log-list" bindtap="clickfn">
